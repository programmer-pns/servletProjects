<?xml version="1.0" encoding="UTF-8"?>

<web-app>
	<welcome-file-list>
		<welcome-file>home.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>homeFile</servlet-name>
		<jsp-file>/home.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>homeFile</servlet-name>
		<url-pattern>/homeurl</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>errorFile</servlet-name>
		<jsp-file>/error.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>errorFile</servlet-name>
		<url-pattern>/errorpageurl</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>successFile</servlet-name>
		<jsp-file>/displayweather.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>successFile</servlet-name>
		<url-pattern>/displayweatherurl</url-pattern>
	</servlet-mapping>
</web-app>