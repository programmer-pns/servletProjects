CREATE TABLE NOTEBOOK_USERS(
USERID NUMBER(6) PRIMARY KEY,
USERNAME VARCHAR2(20) NOT NULL,
EMAIL VARCHAR2(30) NOT NULL UNIQUE,
PASSWORD VARCHAR2(40) NOT NULL);


CREATE TABLE NOTEBOOKS(
NOTEBOOKID NUMBER(5) PRIMARY KEY,
SUBJECT_CODE NUMBER(4) NOT NULL,
USERID NUMBER REFERENCES NOTEBOOK_USERS(USERID) NOT NULL,
NOTEBOOKPATH VARCHAR2(70) NOT NULL
UPLOAD_DATE DATE NOT NULL);


CREATE TABLE NOTEBOOK_SUBJECT_DETAILS(
SUBJECT_CODE VARCHAR2(7) PRIMARY KEY,
SUBJECT_NAME VARCHAR2(30) NOT NULL UNIQUE,
BASE_PATH VARCHAR2(40) NOT NULL);